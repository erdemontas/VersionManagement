@model IEnumerable<VersionManagement.DTOs.ProductDTO>

@{
    ViewData["Title"] = "ProductList";
    var productName = Model.FirstOrDefault().Name;
}
<h1>PRODUCTS</h1>
<table class="table">
    <thead>
        <tr>
            <th>
                Product Name
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model)
{
        <tr>
            <td>
                <a asp-controller="Product" asp-action="Index" asp-route-id="@item.Id">@Html.DisplayFor(modelItem => item.Name)</a>
            </td>
        </tr>
}
    </tbody>
</table>
